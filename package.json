{
  "name": "@no-snapshots/javascript",
  "version": "0.0.1-alpha",
  "description": "JS runtime for no-snapshots",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/no-snapshots/javascript.git"
  },
  "author": "Shubham Naik",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/no-snapshots/javascript/issues"
  },
  "keywords": ["javascript", "testing"],
  "files": ["lib/**/*"],
  "homepage": "https://github.com/no-snapshots/javascript#readme",
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "jest": "^28.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
    "tslint-config-prettier": "^1.18.0"
  }
}
