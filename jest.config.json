{
  "testEnvironment": "node",
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testPathIgnorePatterns": [
    "/lib",
    "/node_modules/"
  ],
  "reporters": ["default"],
  "testRegex": ".*(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
}
